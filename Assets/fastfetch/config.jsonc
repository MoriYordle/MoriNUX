{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "raw",
    "source": "~/.config/fastfetch/xenia.sixel",
    "padding": {
      "right": 35,
      "top": 3
    }
  },
  "general": {
    "multithreading": true
  },
  "display": {
    "separator": "➜   "
  },
  "modules": [
    "break",
    {
      "type": "title",
      "format": "               {8}"
    },
    {
      "type": "custom",
      "format": "┌────────────────────────────────────┐" // `\u001b` is `\033`, or `\e`
    },
    {
      "key": "     ",
      "keyColor": "green",
      "type": "os"
    },
    {
      "key": "     ",
      "keyColor": "blue",
      "type": "kernel"
    },
    {
      "key": "  󰅐   ",
      "keyColor": "green",
      "type": "uptime"
    },
    {
      "key": "     ",
      "keyColor": "cyan",
      "type": "packages",
      "format": "{} (pacman)"
    },
    {
      "key": "     ",
      "keyColor": "blue",
      "type": "de"
    },
    {
      "key": "     ",
      "keyColor": "green",
      "type": "shell"
    },
    {
      "key": "  󰻠   ",
      "keyColor": "green",
      "type": "cpu",
      "format": "{1}"
    },
    {
      "key": "  󰑭   ",
      "keyColor": "blue",
      "type": "memory"
    },
    {
      "type": "custom",
      "format": "└────────────────────────────────────┘" // `\u001b` is `\033`, or `\e`
    },
    {
      "type": "colors",
      "paddingLeft": 11,
      "symbol": "circle",
      "block": {
        "width": 10
      }
    }
  ]
}
